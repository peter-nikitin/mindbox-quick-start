  "scripts": {
    "test": "jest  --maxWorkers=1",
    "test:dev": "jest  --maxWorkers=1 --watch --coverage",
    "start:dev": "nodemon",
    "start": "node ./build/server/index.js",
    "start:front": "webpack --config ./webpack.config.dev.js serve --open",
    "build:front": "webpack --config ./webpack.config.js && gulp",
    "build:back": "tsc --project tsconfic.server.json",
    "build": "npm run build:front && npm run build:back",
    "webpack": "webpack --config ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xNa1D/mindbox-quick-start.git"
  },
  "keywords": [
    "express",
    "nodejs",
    "mindbox"
  ],
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1",
      "^client/(.*)": "<rootDir>/src/client/$1",
      "^server/(.*)": "<rootDir>/src/server/$1"
    }
  },
